
@{
    ViewData["Title"] = "Home Page";
}
<div class="d-flex   justify-content-center" style="min-height: 7vh;">
    <partial name="_WelcomeMessagePartial"></partial>
</div>

<div class="container">
    <div class="row">
        <div class="col-lg-8 col-md-6 col-sm-12 mx-auto my-auto">
            <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel" data-bs-interval="2000">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img src="~/images/carBac1.png" class="d-block w-100" alt="Wild Landscape" />
                    </div>
                    <div class="carousel-item">
                        <img src="~/images/techback-01.png" class="d-block w-100" alt="Camera" />
                    </div>
                    <div class="carousel-item">
                        <img src="~/images/teckstuff-01.png" class="d-block w-100" alt="Exotic Fruits" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="d-flex justify-content-center p-3">
    <partial name="_HomeCategory" model="@ViewData["Categories"]"></partial>
</div>
